package Apllication;

import java.util.Locale;
import java.util.Scanner;

public class Class03double {

	public static void main(String[] args) {
		Locale.setDefaul(Locale.US)
		Scanner sc = new Scanner(System.in);
		
			double x;
			x = sc.nextDouble();
			System.out.println("Voce digitou:" + x);		
		
				
		sc.close();
	
	
	
			
	}

}

